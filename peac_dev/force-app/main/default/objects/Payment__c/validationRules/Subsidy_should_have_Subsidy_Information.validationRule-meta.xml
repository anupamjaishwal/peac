<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Subsidy_should_have_Subsidy_Information</fullName>
    <active>true</active>
    <description>AND(ISBLANK(TEXT(Subsidy_Information__c)) ,
Plus_Subsidy__c  &gt; 0)</description>
    <errorConditionFormula>AND(
$Setup.ORE__c.Bypass__c =False,
ISBLANK(TEXT(Subsidy_Information__c)) ,
Plus_Subsidy__c  &gt; 0
)</errorConditionFormula>
    <errorMessage>Subsidy Information is Required.</errorMessage>
</ValidationRule>
