<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ACH_ACCOUNT_NUMBER</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
!ISBLANK(ACH_Account_Number__c), 
!ISNUMBER(ACH_Account_Number__c)
)</errorConditionFormula>
    <errorDisplayField>ACH_Account_Number__c</errorDisplayField>
    <errorMessage>ACH Account # should be numeric.</errorMessage>
</ValidationRule>
