<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>DefaultPayType</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL(Default_Pay_Type__c,&apos;ACH&apos;),ACH_Available__c == false)</errorConditionFormula>
    <errorDisplayField>Default_Pay_Type__c</errorDisplayField>
    <errorMessage>Default Paytype of ACH Requires ACH to be available.</errorMessage>
</ValidationRule>
