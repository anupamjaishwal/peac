<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>WIRE_ACCOUNT_NUMBER</fullName>
    <active>true</active>
    <errorConditionFormula>AND( 
!ISBLANK(Wire_Account_Number__c), 
!ISNUMBER(Wire_Account_Number__c) 
)</errorConditionFormula>
    <errorDisplayField>Wire_Account_Number__c</errorDisplayField>
    <errorMessage>Wire Account # must be numeric</errorMessage>
</ValidationRule>
