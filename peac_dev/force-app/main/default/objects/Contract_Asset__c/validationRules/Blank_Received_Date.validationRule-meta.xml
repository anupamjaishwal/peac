<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Blank_Received_Date</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISBLANK(Equipment_Received_Date__c),  ISPICKVAL(File_Upload__c, &apos;Condition&apos;))</errorConditionFormula>
    <errorDisplayField>Equipment_Received_Date__c</errorDisplayField>
    <errorMessage>Received date can&apos;t be empty when uploading a Condition file</errorMessage>
</ValidationRule>
