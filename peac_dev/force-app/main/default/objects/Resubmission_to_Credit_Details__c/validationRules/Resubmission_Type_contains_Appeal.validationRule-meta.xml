<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Resubmission_Type_contains_Appeal</fullName>
    <active>true</active>
    <errorConditionFormula>AND(CONTAINS(TEXT(Resubmission_Type_Picklist__c), &apos;Appeal&apos;), ISBLANK(TEXT(Appeal_Type__c)))</errorConditionFormula>
    <errorDisplayField>Appeal_Type__c</errorDisplayField>
    <errorMessage>Please select appeal level</errorMessage>
</ValidationRule>
