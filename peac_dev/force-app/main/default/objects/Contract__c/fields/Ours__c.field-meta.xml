<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Ours__c</fullName>
    <description>DP-162 used to share/display the correct records to portal users.</description>
    <externalId>false</externalId>
    <formula>AND(
				!ISBLANK(Dealer_Account__c),
				Dealer_Account__c =  LEFT($User.Dealer__c, 15)
				)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Ours</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
