    @IsTest static void gdsDealerTest() {
        User sysAdmin = [SELECT Id FROM User WHERE Username = 'service.admin.test.user@silverlinecrm.com'];
        System.runAs(sysAdmin){
            Test.startTest();
            TC_LGHTNPullGDSDealerDNB.res = new MockGDSDealerResponse().respond(null);
            Account acct = [SELECT Id FROM Account WHERE Name = :companyName LIMIT 1];
            acct.Dealer_Number__c = dealerNumber;
            acct.Review_For__c = 'Approval';
            acct.DM_Review_Status__c = 'Submitted for Review';
            update acct;
            TC_GDSDealerQueueable dq = new TC_GDSDealerQueueable(acct.Id, Label.TC_GDS_Get_Dealer);
            dq.execute(null);
            Test.stopTest();
            System.assertEquals(RESPONSEDATA_DNBDVPACKET_SBRILEASESCORE, [SELECT Id, SBRI__c FROM Account WHERE Id = :acct.Id].SBRI__c,
             'The SBRI Score was not mapped from the response!');
        }
    }

    @IsTest static void infoLeaseResponseTest() {
        Test.startTest();
        Test.setMock(HttpCalloutMock.class, new MockDealerInfoLeaseResponse());
        Account acct = [SELECT Id FROM Account WHERE Name = :companyName LIMIT 1];
        TC_GDSDealerQueueable dq = new TC_GDSDealerQueueable(acct.Id, Label.TC_GDS_Get_Dealer_InfoLease);
        dq.execute(null);
        Test.stopTest();
        System.assert(String.isNotBlank([SELECT Id, Dealer_Number__c FROM Account WHERE Id = :acct.Id].Dealer_Number__c), 'The dealer number was not set!');
